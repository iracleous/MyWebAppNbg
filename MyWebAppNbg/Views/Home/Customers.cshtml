@{
}

@model IEnumerable<DomainProject.Models.Customer>

 


@if (Model != null)
{
    <table id ="mytable" ><tr><th>Id</th><th>Name</th></tr>
    @foreach (var item in Model)
    {
            <tr >
                <td> @item.Id  </td>
                <td> @item.Name </td>
            </tr>
    }
  </table>

}     